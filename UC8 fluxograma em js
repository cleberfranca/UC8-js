    console.log("Idade inválida, Cadastro não permitido")
}

let cadastro = 178
if(cadastro <= 100){
    console.log(" Cadastro realizado com sucesso!")
} else {
    console.log("Desculpe, cadastro não é permitido, excedido o limite de participantes.")
}

let participante = 98
if(participante <= 100){
    console.log("mostrar lista de participantes e palestrantes cadastrados!")
